<h1>Show All Employee</h1>

<table class="table table-hover">
  <thead>
    <tr>
      <th scope="col">Name</th>
      <th scope="col">Email</th>
      <th scope="col">Address</th>
      <th scope="col">Pno</th>
      <th scope="col">Edit</th>
    </tr>
  </thead>
  <tbody>
      {{#each list}}
    <tr class="table-active">
      <th scope="row">{{this.empname}}</th>
      <td>{{this.empmail}}</td>
      <td>{{this.empaddress}}</td>
      <td>{{this.empno}}</td>
      <td><a href="/empupdate/{{this.empid}}" class="btn btn-success">Edit</a></td>
      <td><a href="/deleteupdate/{{this.empid}}" class="btn btn-danger">Delete</a></td>
    </tr>
    {{/each}}
  </tbody>
</table>